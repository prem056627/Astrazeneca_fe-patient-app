{
  "expo": {
    "name": "Newmi Care",
    "slug": "newmi-app",
    "version": "1.1.3",
    "extra": {
      "eas": {
        "projectId": "c4957b2c-b721-47a9-89ec-f7b71089eca4"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/newmi-icon-new.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/login.png",
      "backgroundColor": "#A11327",
      "resizeMode": "contain"
    },
    "ios": {
      "buildNumber": "16",
      "supportsTablet": true,
      "bundleIdentifier": "com.kahealthcare.newmicare1",
      "splash": {
        "image": "./assets/login.png",
        "resizeMode": "contain",
        "backgroundColor": "#A11327"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera to let you share invoices, health records, medicine details, prescription in form submission.",
        "NSPhotoLibraryUsageDescription": "$(PRODUCT_NAME) needs access to your Photos to let you share invoices, health records, medicine details, prescription in form submission."
      },
      "appleTeamId": "Z693JX4A9W"
    },
    "android": {
      "versionCode": 16,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "splash": {
        "image": "./assets/splash_android.png",
        "resizeMode": "center",
        "backgroundColor": "#A11327"
      },
      "package": "com.kahealthcare.newmicare1",
      "jsEngine": "hermes"
    },
    "scheme": "com.kahealthcare.newmicare1",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "zelthy"
  }
}
